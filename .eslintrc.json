{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true,
        "node": true,
        "jest": true
    },
    "settings": {
        "react": {
            "version": "17.0.2"
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y"
    ],
    "rules": {
    }
}
